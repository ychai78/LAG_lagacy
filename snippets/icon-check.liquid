{% comment %}
    Renders a check icon

    Usage:
       {% render 'icon-check',  class: 'w-[75px] h-[75px]' %}
    %}
{% endcomment %}



{%- liquid
   if class == nil
   assign class =  'size-6' 
   else
    assign class = class 
endif
-%}



